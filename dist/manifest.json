{
  "name": "Policy Hub",
  "manifest_version": 3,
  "version": "1.0",
  "description": "Helps you manage your cookies and policies",
  "permissions": ["cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
